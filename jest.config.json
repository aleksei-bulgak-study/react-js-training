{
  "transform": {
    "^.+\\.(js|jsx)$": "babel-jest"
  },
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "moduleNameMapper": {
    "\\.(css)$": "<rootDir>/__mocks__/styleMock.js"
  },
  "setupFilesAfterEnv": [
    "<rootDir>/__tests__/configs/enzyme.config.js"
  ],
  "modulePathIgnorePatterns": ["<rootDir>/__tests__/configs"]
}
